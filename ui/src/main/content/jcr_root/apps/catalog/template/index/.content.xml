<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:description="Index Page"
    jcr:primaryType="cq:Template"
    jcr:title="Catalog Index Page"
    allowedPaths="[/content/[^/]+(/.*)?]"
    ranking="{Long}100">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="/apps/catalog/components/structure/index">
        <header
            jcr:primaryType="nt:unstructured"
            sling:resourceType="foundation/components/reference"
            path="/content/geometrixx-outdoors/en/men/jcr:content/sidebar/list"/>
        <par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="foundation/components/parsys"/>
        <footer
            jcr:primaryType="nt:unstructured"
            sling:resourceType="foundation/components/reference"
            path="/content/geometrixx-outdoors/en/men/jcr:content/sidebar/list"/>
    </jcr:content>
</jcr:root>
