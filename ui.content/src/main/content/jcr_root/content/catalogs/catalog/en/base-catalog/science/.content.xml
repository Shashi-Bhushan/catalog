            jcr:primaryType="nt:unstructured"
            cq_designPath="/etc/designs/catalog-design"
            jcr_title="Science"/>
        <filter
            jcr:primaryType="nt:unstructured"
            matchTags="[catalog:science]"/>
    </jcr:content>
</jcr:root>
