            jcr:primaryType="nt:unstructured"
            product="/content/catalogs/catalog/en/template-pages/catalog-product-template"
            proxyProductPages="parseFloat"
            proxyProductPagesSelection="inherit"
            section="/content/catalogs/catalog/en/template-pages/catalog-product-listing-template"/>
